diff --git a/server/migrations/1733055231222_init.ts b/server/migrations/1733055231222_init.ts
new file mode 100644
index 0000000..2e3f263
--- /dev/null
+++ b/server/migrations/1733055231222_init.ts
@@ -0,0 +1,33 @@
+import { Kysely, sql } from "kysely";
+
+export async function up(db: Kysely<any>): Promise<void> {
+  await db.schema
+    .createTable("sports")
+    .addColumn("id", "serial", (col) => col.primaryKey())
+    .addColumn("name", "text")
+    .addColumn("created_at", "timestamp", (col) =>
+      col.defaultTo(sql`now()`).notNull()
+    )
+    .addColumn("updated_at", "timestamp", (col) =>
+      col.defaultTo(sql`now()`).notNull()
+    )
+    .execute();
+
+  await db.schema
+    .createTable("users")
+    .addColumn("id", "serial", (col) => col.primaryKey())
+    .addColumn("name", "text", (col) => col.notNull())
+    .addColumn("hashed_password", "text", (col) => col.notNull())
+    .addColumn("created_at", "timestamp", (col) =>
+      col.defaultTo(sql`now()`).notNull()
+    )
+    .addColumn("updated_at", "timestamp", (col) =>
+      col.defaultTo(sql`now()`).notNull()
+    )
+    .execute();
+}
+
+export async function down(db: Kysely<any>): Promise<void> {
+  await db.schema.dropTable("sports").execute();
+  await db.schema.dropTable("users").ifExists().execute();
+}
